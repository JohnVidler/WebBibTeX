{
	"manifest_version": 2,

	"name": "WebBibtex",
	"description": "Binds web addresses to Bibtex entries",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"activeTab",
		"http://apps.johnvidler.co.uk/webbibtex/*",
		"http://johnvidler.co.uk/apps/webbibtex/*"
	]
}